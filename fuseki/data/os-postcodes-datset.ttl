<https://www.data.gov.uk/dataset/2dfb82b4-741a-4b93-807e-11abb4bb0875/os-postcodes-data> a dcat:Dataset ;
    dct:identifier "2dfb82b4-741a-4b93-807e-11abb4bb0875" ;
    dct:accessRights "OPEN" ;
    dct:accrualPeriodicity freq:quarterly ;
    dct:creator "ordnance-survey" ;
    dct:description "The United Kingdom's (England, Scotland, Wales, Northern Ireland) postcodes represented as points and polygons. This data is often used for service delivery and statistical analysis. OS Postcodes Data is made available in a number of Ordnance Survey Products: Codepoint (point geometry), Codepoint Open (point geometry) and Codepoint with Polygons (polygon geometry). The postcode is sourced from Royal Mail. The polygon geometry is calculated using address geometry provided by Local Authority Land and Property Gazetteer custodians as a base." ;
    dct:issued "2022-02-01"^^xsd:date ;
    dct:license "http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" ;
    dct:modified "2023-02-01"^^xsd:date ;
